inputstr = "Surendra"
#print(inputstr[-1: : -1])
result = ''
for i in range(len(inputstr)-1, -1, -1):
    result = result + inputstr[i]
print(result)


a = int(input("Enter the number: "))
rev = 0
while(a > 0):
    dig = a%10
    rev = rev*10+dig
    a = a//10

print("Reverse of the number: ", rev)
